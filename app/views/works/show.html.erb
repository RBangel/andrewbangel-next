<div id="works">
	<h2><%= @work.title %></h2>
	<div class="flex-video widescreen vimeo">
		<iframe width="420" height="315" src="<%= @work.url %>" frameborder="0" allowfullscreen></iframe>
	</div>
	<hr>
	<table>
		<tr>
			<td>Type</td><td><%= @work.work_type.name %></td>
		</tr>
		<tr>
			<td>url</td><td><%= @work.url %></td>
		</tr>
		<tr>
			<td>Thumbnail</td><td><%= @work.thumbnail %></td>
		</tr>
		<tr>
			<td>Date Created</td><td><%= @work.date_created %></td>
		</tr>
		<tr>
			<td>Record Added</td><td><%= @work.created_at %></td>
		</tr>
		<tr>
			<td>Record Modified</td><td><%= @work.updated_at %></td>
		</tr>
	</table>
</div>
